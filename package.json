{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "description": "QuickStart Angular2 with webpack quick start + webpack-dev-server, supplemented with testing support. Use node v4 & npm v3. 'npm install' then 'npm start'. Run npm start again after npm run build.",
  "scripts": {
    "start": "npm run cleandev && webpack-dev-server",
    "test": "karma start",
    "build": "npm run cleandev && npm run cleanprod && webpack --config config/webpack.prod.js --progress --profile --bail",
    "cleandev": "rimraf ./src/{view}/**/*.{js,js.map} && rimraf ./src/*.{js,js.map}",
    "cleanprod":"rimraf ./dist"
  },
